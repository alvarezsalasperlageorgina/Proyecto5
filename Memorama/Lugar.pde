interface Lugar {
  void display ();
  void display2 ();
  int lx ();
  int ly ();
  int lposicion ();
}

  class Lugar1 implements Lugar {
  int x;
  int y;
  int posicion;
  
  Lugar1 (int x_, int y_, int posicion_) {
    x = x_;
    y = y_;
    posicion = posicion_;
  }
  
  void display () {
    noStroke ();
    fill (200);
    rect (x, y, 150, 150);
  }
  
  void display2 () {
    noStroke ();
    fill (0);
    rect (x, y, 150, 150);
  }
  
  int lx () {
    return x;
  }
  
  int ly () {
    return y;
  }
  
  int lposicion () {
    return posicion;
  }
}

